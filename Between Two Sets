using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;

class Result
{
    public static int getTotalX(List<int> a, List<int> b)
    {
        int count = 0;

        // x should be between the max of a and min of b
        int start = a.Max();
        int end = b.Min();

        for (int x = start; x <= end; x++)
        {
            bool valid = true;

            // Check if all a[i] divides x
            foreach (int num in a)
            {
                if (x % num != 0)
                {
                    valid = false;
                    break;
                }
            }

            // Check if x divides all b[j]
            if (valid)
            {
                foreach (int num in b)
                {
                    if (num % x != 0)
                    {
                        valid = false;
                        break;
                    }
                }
            }

            if (valid)
            {
                count++;
            }
        }

        return count;
    }
}

class Solution
{
    public static void Main(string[] args)
    {
        TextWriter textWriter = new StreamWriter(@System.Environment.GetEnvironmentVariable("OUTPUT_PATH"), true);

        string[] firstMultipleInput = Console.ReadLine().TrimEnd().Split(' ');

        int n = Convert.ToInt32(firstMultipleInput[0]);
        int m = Convert.ToInt32(firstMultipleInput[1]);

        List<int> arr = Console.ReadLine().TrimEnd().Split(' ').ToList().Select(int.Parse).ToList();
        List<int> brr = Console.ReadLine().TrimEnd().Split(' ').ToList().Select(int.Parse).ToList();

        int total = Result.getTotalX(arr, brr);

        textWriter.WriteLine(total);

        textWriter.Flush();
        textWriter.Close();
    }
}
